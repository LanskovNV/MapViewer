{
  "name": "MapViewer",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.3.1",
    "husky": "^1.3.1",
    "immutable": "3.8.2",
    "intl": "1.2.5",
    "lint-staged": "^8.1.5",
    "loadable-components": "2.2.3",
    "prettier": "^1.16.4",
    "prop-types": "15.6.2",
